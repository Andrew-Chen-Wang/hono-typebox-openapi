{
  "name": "hono-typebox-openapi",
  "version": "0.0.2",
  "description": "OpenAPI schema generator for Hono and TypeBox",
  "type": "module",
  "main": "dist/index.cjs",
  "module": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": ["dist"],
  "scripts": {
    "build": "pkgroll --save-dist"
  },
  "keywords": ["hono", "openapi", "typebox"],
  "homepage": "https://github.com/Andrew-Chen-Wang/hono-typebox-openapi",
  "publishConfig": {
    "access": "public"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Andrew-Chen-Wang/hono-typebox-openapi.git",
    "directory": "packages/hono-typebox-openapi"
  },
  "bugs": {
    "url": "https://github.com/Andrew-Chen-Wang/hono-typebox-openapi/issues"
  },
  "author": "Andrew-Chen-Wang",
  "license": "MIT",
  "private": false,
  "dependencies": {
    "json-schema-walker": "^2.0.0"
  },
  "peerDependencies": {
    "openapi-types": "^12.1.3",
    "typebox": "^1.0.0"
  },
  "peerDependenciesMeta": {
    "@hono/typebox-validator": {
      "optional": true
    },
    "typebox": {
      "optional": true
    },
    "hono": {
      "optional": true
    }
  },
  "devDependencies": {
    "@apidevtools/json-schema-ref-parser": "^12.0.2",
    "@rollup/plugin-terser": "^0.4.4",
    "@types/json-schema": "^7.0.15",
    "pkgroll": "^2.20.1"
  },
  "exports": {
    ".": {
      "import": {
        "types": "./dist/index.d.ts",
        "default": "./dist/index.js"
      },
      "require": {
        "types": "./dist/index.d.cts",
        "default": "./dist/index.cjs"
      }
    },
    "./typebox": {
      "import": {
        "types": "./dist/typebox.d.ts",
        "default": "./dist/typebox.js"
      },
      "require": {
        "types": "./dist/typebox.d.cts",
        "default": "./dist/typebox.cjs"
      }
    }
  }
}
